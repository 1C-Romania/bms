			<Attribute name="List" id="1">
				<Type>
					<v8:Type>cfg:DynamicList</v8:Type>
				</Type>
				<MainAttribute>true</MainAttribute>
				<Settings xsi:type="DynamicList">
					<ManualQuery>true</ManualQuery>
					<DynamicDataRead>true</DynamicDataRead>
					<QueryText>SELECT
	DocumentJournalDocumentsOnPersonnel.Ref,
	DocumentJournalDocumentsOnPersonnel.Date,
	DocumentJournalDocumentsOnPersonnel.DeletionMark,
	DocumentJournalDocumentsOnPersonnel.Number,
	DocumentJournalDocumentsOnPersonnel.Posted,
	DocumentJournalDocumentsOnPersonnel.Entity,
	DocumentJournalDocumentsOnPersonnel.Comment,
	DocumentJournalDocumentsOnPersonnel.Type
FROM
	DocumentJournal.HRMDocuments AS DocumentJournalDocumentsOnPersonnel
WHERE
	&amp;Employee = VALUE(Catalog.Employees.EmptyRef)
			OR DocumentJournalDocumentsOnPersonnel.Ref IN
				(SELECT
					EmploymentEmployees.Ref AS Ref
				FROM
					Document.Employment.Employees AS EmploymentEmployees
				WHERE
					EmploymentEmployees.Employee = &amp;Employee
		
				UNION ALL
		
				SELECT
					StaffDisplacementEmployees.Ref
				FROM
					Document.EmployeeStatusChanges.Employees AS StaffDisplacementEmployees
				WHERE
					StaffDisplacementEmployees.Employee = &amp;Employee
		
				UNION ALL
		
				SELECT
					DismissalStaff.Ref
				FROM
					Document.Dismissal.Employees AS DismissalStaff
				WHERE
					DismissalStaff.Employee = &amp;Employee)</QueryText>
					<MainTable>DocumentJournal.HRMDocuments</MainTable>
					<ListSettings>
						<dcsset:filter>
							<dcsset:viewMode>Normal</dcsset:viewMode>
							<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
						</dcsset:filter>
						<dcsset:order>
							<dcsset:viewMode>Normal</dcsset:viewMode>
							<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
						</dcsset:order>
