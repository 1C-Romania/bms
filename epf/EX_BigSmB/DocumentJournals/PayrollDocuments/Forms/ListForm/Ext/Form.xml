	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentJournalPayrollDocuments.Ref,
	DocumentJournalPayrollDocuments.Date,
	DocumentJournalPayrollDocuments.DeletionMark,
	DocumentJournalPayrollDocuments.Number,
	DocumentJournalPayrollDocuments.Posted,
	DocumentJournalPayrollDocuments.Entity,
	DocumentJournalPayrollDocuments.RegistrationPeriod,
	DocumentJournalPayrollDocuments.Department,
	DocumentJournalPayrollDocuments.TransactionType,
	DocumentJournalPayrollDocuments.Amount,
	DocumentJournalPayrollDocuments.Currency,
	DocumentJournalPayrollDocuments.Comment,
	DocumentJournalPayrollDocuments.Type
FROM
	DocumentJournal.PayrollDocuments AS DocumentJournalPayrollDocuments
WHERE
	(&amp;Employee = VALUE(Catalog.Employees.EmptyRef)
			OR DocumentJournalPayrollDocuments.Ref IN
				(SELECT
					NestedSelect.Document AS Ref
				FROM
					(SELECT
						PayrollAccrualRetention.Ref AS Document
					FROM
						Document.Payroll.AccrualsDeductions AS PayrollAccrualRetention
					WHERE
						PayrollAccrualRetention.Employee = &amp;Employee
		
					UNION ALL
		
					SELECT
						TimesheetTimeWorkedByDay.Ref
					FROM
						Document.Timesheet.TimeWorkedByDays AS TimesheetTimeWorkedByDay
					WHERE
						TimesheetTimeWorkedByDay.Employee = &amp;Employee
		
					UNION ALL
		
					SELECT
						TimesheetTimeWorkedPerPeriod.Ref
					FROM
						Document.Timesheet.TimeWorkedPerPeriod AS TimesheetTimeWorkedPerPeriod
					WHERE
						TimesheetTimeWorkedPerPeriod.Employee = &amp;Employee
		
					UNION ALL
		
					SELECT
						PayrollSheetEmployees.Ref
					FROM
						Document.PayrollSheet.Employees AS PayrollSheetEmployees
					WHERE
						PayrollSheetEmployees.Employee = &amp;Employee
					) AS NestedSelect))</QueryText>
				<MainTable>DocumentJournal.PayrollDocuments</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
				</ListSettings>
			</Settings>
