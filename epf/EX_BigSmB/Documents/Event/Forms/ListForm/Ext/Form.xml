					<QueryText>SELECT
	CASE
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.PhoneCall)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Incoming)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 17
					ELSE 7
				END
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.PhoneCall)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Outgoing)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 12
					ELSE 2
				END
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.PrivateMeeting)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Incoming)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 15
					ELSE 5
				END
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.PrivateMeeting)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Outgoing)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 10
					ELSE 0
				END
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.ELetter)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Incoming)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 18
					ELSE 8
				END
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.ELetter)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Outgoing)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 13
					ELSE 3
				END
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.MailLetter)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Incoming)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 19
					ELSE 9
				END
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.MailLetter)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Outgoing)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 14
					ELSE 4
				END
		WHEN DocumentEvent.EventType = VALUE(Enum.EventTypes.Other)
				AND DocumentEvent.IncomingOutgoingEvent = VALUE(Enum.IncomingOutgoingEvent.Incoming)
			THEN CASE
					WHEN DocumentEvent.DeletionMark
						THEN 16
					ELSE 6
				END
		ELSE CASE
				WHEN DocumentEvent.DeletionMark
					THEN 11
				ELSE 1
			END
	END AS PictureNo,
	DocumentEvent.Ref AS Ref,
	DocumentEvent.DeletionMark AS DeletionMark,
	DocumentEvent.Number AS Number,
	DocumentEvent.Date AS Date,
	DocumentEvent.Posted AS Posted,
	DocumentEvent.EventType AS EventType,
	DocumentEvent.State AS Status,
	DocumentEvent.Counterparty AS Counterparty,
	DocumentEvent.Subject AS Subject,
	DocumentEvent.Content AS Content,
	DocumentEvent.DocumentBasis AS DocumentBasis,
	DocumentEvent.Project AS Project,
	DocumentEvent.EventBegin AS EventBegin,
	DocumentEvent.EventEnding AS EventEnding,
	DocumentEvent.Author AS Author,
	DocumentEvent.Responsible AS Responsible,
	DocumentEvent.Parties.(
		Ref,
		LineNumber,
		ContactPresentation,
		HowToContact,
		ContactPerson
	) AS Parties,
	DocumentEvent.PointInTime AS PointInTime,
	DocumentEvent.MembersList AS MembersList,
	DocumentEvent.IncomingOutgoingEvent AS IncomingOutgoing
FROM
	Document.Event AS DocumentEvent
WHERE
	(&amp;ContactFilled
				AND DocumentEvent.Parties.ContactPerson = &amp;ContactPerson
			OR NOT &amp;ContactFilled)</QueryText>
