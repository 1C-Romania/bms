	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentProductionOrder.Ref,
	DocumentProductionOrder.DataVersion,
	DocumentProductionOrder.DeletionMark,
	DocumentProductionOrder.Number,
	DocumentProductionOrder.Date,
	DocumentProductionOrder.Posted,
	DocumentProductionOrder.TransactionType,
	DocumentProductionOrder.Entity,
	DocumentProductionOrder.CustomerOrder,
	DocumentProductionOrder.Start,
	DocumentProductionOrder.Finish,
	DocumentProductionOrder.BaseUnit,
	DocumentProductionOrder.Comment,
	DocumentProductionOrder.BaseUnitReserve,
	DocumentProductionOrder.DocumentBasis,
	DocumentProductionOrder.StatusOfOrder,
	CASE
		WHEN (DocumentProductionOrder.StatusOfOrder.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR DocumentProductionOrder.StatusOfOrder.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND (NOT DocumentProductionOrder.Closed)
			THEN "In process"
		ELSE CASE
				WHEN DocumentProductionOrder.StatusOfOrder.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN "Completed"
				ELSE "Cancelled"
			END
	END AS OrderStatus,
	DocumentProductionOrder.Closed,
	DocumentProductionOrder.Responsible,
	DocumentProductionOrder.Author,
	DocumentProductionOrder.NomenclatureList,
	DocumentProductionOrder.ResourcesList,
	DocumentProductionOrder.Inventory.(
		Ref,
		LineNumber,
		Nomenclature,
		Characteristic,
		Quantity,
		UnitOfMeasure,
		Specification,
		Reserve
	),
	DocumentProductionOrder.EnterpriseResources.(
		Ref,
		LineNumber,
		EnterpriseResource,
		Capacity,
		Duration,
		Start,
		Finish
	),
	DocumentProductionOrder.PointInTime
FROM
	Document.ProductionOrder AS DocumentProductionOrder</QueryText>
