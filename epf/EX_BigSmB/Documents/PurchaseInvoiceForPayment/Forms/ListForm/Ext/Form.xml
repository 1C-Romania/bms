	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentInvoiceForPayment.Ref,
	DocumentInvoiceForPayment.DeletionMark,
	DocumentInvoiceForPayment.Number,
	DocumentInvoiceForPayment.Date,
	DocumentInvoiceForPayment.Posted,
	DocumentInvoiceForPayment.Entity,
	DocumentInvoiceForPayment.Comment,
	DocumentInvoiceForPayment.DocumentBasis,
	DocumentInvoiceForPayment.Counterparty,
	DocumentInvoiceForPayment.Contract,
	DocumentInvoiceForPayment.CashType,
	DocumentInvoiceForPayment.BankAccount,
	DocumentInvoiceForPayment.DocumentCurrency,
	DocumentInvoiceForPayment.Multiplicity,
	DocumentInvoiceForPayment.ExchangeRate,
	DocumentInvoiceForPayment.AmountIncludesVAT,
	DocumentInvoiceForPayment.CounterpartyPriceKinds,
	DocumentInvoiceForPayment.DocumentAmount,
	DocumentInvoiceForPayment.PettyCash,
	DocumentInvoiceForPayment.Responsible,
	DocumentInvoiceForPayment.PointInTime,
	DocumentInvoiceForPayment.Author,
	DocumentInvoiceForPayment.IncomingDocumentNo,
	DocumentInvoiceForPayment.DateOfIncomingDocument,
	InvoicesAndOrdersPaymentTurnovers.AmountOfPaymentTurnover
FROM
	Document.PurchaseInvoiceForPayment AS DocumentInvoiceForPayment
		LEFT JOIN AccumulationRegister.InvoicesAndOrdersPayment.Turnovers AS InvoicesAndOrdersPaymentTurnovers

		ON DocumentInvoiceForPayment.Ref = InvoicesAndOrdersPaymentTurnovers.InvoiceForPayment</QueryText>
				<MainTable>Document.PurchaseInvoiceForPayment</MainTable>
